      return [
        { id: 'torque_wrench', name: 'Torque Wrench', manufacturer: 'Snap-on', model: 'TechAngle', range: '0-250 ft-lb', accuracy: '¬±2%', testPoints: [0, 50, 100, 150, 200, 250], icon: 'fas fa-wrench' },
