{
  "name": "calpro-calibration-system",
  "version": "2.0.0",
  "description": "CalPro - Professional Calibration Management System",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "tsc && vite build",
    "preview": "vite preview",
    "test": "vitest",
    "test:ui": "vitest --ui",
    "test:coverage": "vitest --coverage",
    "type-check": "tsc --noEmit",
    "lint": "eslint . --ext .ts,.tsx,.js,.jsx",
    "lint:fix": "eslint . --ext .ts,.tsx,.js,.jsx --fix",
    "format": "prettier --write \"**/*.{ts,tsx,js,jsx,json,css,md}\"",
    "deploy": "npm run build",
    "deploy:railway": "railway up"
  },
  "keywords": [
    "calibration",
    "instruments",
    "certificates",
    "offline-first",
    "PWA",
    "quality-control",
    "frontend"
  ],
  "author": "Jay Monty",
  "license": "MIT",
  "engines": {
    "node": ">=18.0.0"
  },
  "homepage": "https://calpro-calibration.netlify.app",
  "repository": {
    "type": "git",
    "url": "https://github.com/jaymonty/calpro-calibration-system.git"
  },
  "devDependencies": {
    "@types/node": "^20.10.0",
    "@typescript-eslint/eslint-plugin": "^6.13.0",
    "@typescript-eslint/parser": "^6.13.0",
    "@vitest/ui": "^1.0.0",
    "@vitest/coverage-v8": "^1.0.0",
    "eslint": "^8.54.0",
    "eslint-config-prettier": "^9.0.0",
    "eslint-plugin-prettier": "^5.0.0",
    "jsdom": "^23.0.0",
    "prettier": "^3.1.0",
    "typescript": "^5.3.0",
    "vite": "^5.0.0",
    "vitest": "^1.0.0"
  },
  "dependencies": {
    "jspdf": "^2.5.1"
  }
}
